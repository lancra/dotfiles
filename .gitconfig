[clone]
	defaultRemoteName = origin
[commit]
	gpgSign = true
[core]
	autocrlf = false
	editor = 'c:/Program Files/Vim/vim91/vim.exe'
	symlinks = true
[diff]
	algorithm = histogram
	colorMoved = default
	tool = kdiff3
[difftool]
	prompt = false
[difftool "meld"]
	cmd = meld "$LOCAL" "$REMOTE"
[fetch]
	prune = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[include]
	path = ~/.config/git/alias.gitconfig
	path = ~/.config/git/delta.gitconfig
[init]
	defaultBranch = main
	defaultObjectFormat = sha1
	defaultRefFormat = files
[merge]
	conflictStyle = zdiff3
	tool = kdiff3
[mergetool]
	keepBackup = false
	prompt = false
[mergetool "meld"]
	cmd = meld "$LOCAL" "$MERGED" "$REMOTE" --output "$MERGED"
	useAutoMerge = true
[push]
	autoSetupRemote = true
	default = current
[rebase]
	autosquash = true
	updateRefs = true
[tag]
	gpgSign = true
[user]
	email = lancra@outlook.com
	name = Lance Craig
	signingKey = lancra@outlook.com
[winUpdater]
	recentlySeenVersion = 2.24.1.windows.2
