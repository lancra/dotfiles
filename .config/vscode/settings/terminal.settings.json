{
  "$schema": "vscode://schemas/settings/user",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "CaskaydiaCove NF Mono",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "%SYSTEMROOT%\\Sysnative\\cmd.exe",
        "%SYSTEMROOT%\\System32\\cmd.exe",
      ],
    },
    "Git Bash": {
      "source": "Git Bash",
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell",
    },
    "Windows PowerShell": {
      "path": "%SYSTEMROOT%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    },
  },
}
