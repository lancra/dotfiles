    {
      "id": "User.find",
      "keys": "ctrl+shift+f"
    },
    {
      "id": "User.scrollToMark.next",
      "keys": "alt+down"
    },
    {
      "id": "User.scrollToMark.previous",
      "keys": "alt+up"
    },
    {
      "id": "User.selectCommand.next",
      "keys": "ctrl+alt+l"
    },
    {
      "id": "User.selectCommand.prev",
      "keys": "ctrl+alt+h"
    },
    {
      "id": "User.selectOutput.next",
      "keys": "ctrl+alt+j"
    },
    {
      "id": "User.selectOutput.prev",
      "keys": "ctrl+alt+k"
    },
    {
      "id": "User.splitPane.duplicate",
      "keys": "alt+shift+d"
    }
  ],
