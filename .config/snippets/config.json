{
  "editors": [
    {
      "key": "vim",
      "name": "VIM",
      "targetDirectory": "~/.config/vim/UltiSnips",
      "scopes": {
        "csharp": {
          "override": "cs"
        },
        "git-commit": {
          "override": "gitcommit"
        },
        "powershell": {
          "override": "ps1"
        }
      },
      "ignoreUndefinedScopes": false
    },
    {
      "key": "vs",
      "name": "Visual Studio",
      "targetDirectory": "~/Documents/Visual Studio 18/Code Snippets/Visual C#/My Code Snippets",
      "scopes": {
        "csharp": {}
      },
      "ignoreUndefinedScopes": true
    },
    {
      "key": "vscode",
      "name": "Visual Studio Code",
      "targetDirectory": "~/AppData/Roaming/Code/User/snippets",
      "ignoreUndefinedScopes": false
    }
  ]
}
