{
  "editors": [
    {
      "key": "ads",
      "name": "Azure Data Studio",
      "targetDirectory": "~/AppData/Roaming/azuredatastudio/User/snippets",
      "scopes": [
        "loganalytics",
        "sql"
      ]
    },
    {
      "key": "vim",
      "name": "VIM",
      "targetDirectory": "~/.config/vim/UltiSnips",
      "scopeOverrides": {
        "csharp": "cs",
        "git-commit": "gitcommit",
        "powershell": "ps1"
      }
    },
    {
      "key": "vs",
      "name": "Visual Studio",
      "targetDirectory": "~/Documents/Visual Studio 18/Code Snippets/Visual C#/My Code Snippets",
      "scopes": [
        "csharp"
      ]
    },
    {
      "key": "vscode",
      "name": "Visual Studio Code",
      "targetDirectory": "~/AppData/Roaming/Code/User/snippets"
    }
  ]
}
