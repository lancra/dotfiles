{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#b9fbc0",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#98f5e1",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#8eecf5",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#90dbf4",
          "background_templates": ["{{ if gt .Code 0 }}#ff8080{{ end }}"],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "background": "#a3c4f3",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uE77F {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "#cfbaf0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f1c0e8{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ffcfd2{{ end }}",
            "{{ if gt .Ahead 0 }}#ffcfd2{{ end }}",
            "{{ if gt .Behind 0 }}#ffcfd2{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ if not .UpstreamGone }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "background": "#fde4cf",
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          },
          "style": "powerline",
          "template": "\ufbab {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#fbf8cc",
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "powerline",
          "template": "\uf64f {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#b9fbc0",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "secondary_prompt": {
    "foreground": "#b9fbc0",
    "template": "\u276f\u276f "
  },
  "version": 2
}
